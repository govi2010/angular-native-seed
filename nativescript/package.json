  "scripts": {
    "prepPhone": "gulp build.Phone",
    "prepTablet": "gulp build.Default",
    "prepCLIPhone": "gulp build.cli.Phone",
    "prepCLITablet": "gulp build.cli.Default",
    "ios": "npm run prepCLITablet && tns run ios",
    "ios.phone": "npm run prepCLIPhone && tns run ios",
    "android": "npm run prepCLITablet && tns run android",
    "android.phone": "npm run prepCLIPhone && tns run android",
    "debug:android": "npm run prepCLITablet && tns debug android --chrome",
    "phone-ios-bundle": "npm run prepPhone && tns prepare ios && tns run ios --bundle --env.aot --env.uglify",
    "tablet-ios-bundle": "npm run prepTablet && tns prepare ios && tns run ios --bundle --env.aot --env.uglify",
    "build.phone-ios-bundle": "npm run prepPhone && tns prepare ios && tns build ios --bundle --env.aot --env.uglify",
    "build.tablet-ios-bundle": "npm run prepTablet && tns prepare ios && tns build ios --bundle --env.aot --env.uglify",
    "phone-android-bundle": "npm run prepPhone && tns prepare android && tns run android --bundle --env.aot --env.uglify",
    "tablet-android-bundle": "npm run prepTablet && tns prepare android && tns run android --bundle --env.aot --env.uglify",
    "build.phone-android-bundle": "npm run prepPhone && tns prepare android && tns build android --bundle --env.aot --env.uglify",
    "build.tablet-android-bundle": "npm run prepTablet && tns prepare android && tns build android --bundle --env.aot --env.uglify",
    "livesync": "gulp tns.Livesync",
    "livesync.phone": "gulp tns.Livesync.Phone",
    "publish-ios-bundle": "tns publish ios --bundle --env.aot --env.uglify"
  }
